                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "Zfc/Admin",
                "version": "0.0.1",
                "source": {
                    "type": "git",
                    "url": "https://github.com/ZF-Commons/ZfcAdmin.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "Bjy/Authorize",
                "version": "1.4",
                "source": {
                    "type": "git",
                    "url": "https://github.com/bjyoungblood/BjyAuthorize.git",
                    "reference": "master"
                }
            }
        }
    ]
